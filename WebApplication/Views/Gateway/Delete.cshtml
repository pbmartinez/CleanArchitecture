@model Application.Dtos.GatewayDto

@{
    ViewData["Title"] = "Borrar Gateway";
}

<div class="text-center">
    <h1 class="display-4">@string.Format(Domain.Localization.Resource.display_DetailsOf,"") Gateway</h1>
    <h3 class="display-4">@Domain.Localization.Resource.display_ConfirmOnDelete</h3>
</div>

<div class="row">
    <div class="col-12  col-md-4">
        <b><label asp-for="Name"></label> </b>
        @Model.Name
    </div>
    <div class="col-12  col-md-4">
        <b><label asp-for="Ipv4Address"></label> </b>
        @Model.Ipv4Address
    </div>
</div>
<div class="row">
    <a class="btn btn-secondary" href="/Gateway/Index">@Domain.Localization.Resource.btn_Cancel</a>
    <a class="btn btn-danger" href="/Gateway/DeleteConfirm/@Model.Id">@Domain.Localization.Resource.btn_Delete</a>
</div>